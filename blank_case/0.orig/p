FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    //- Set patchGroups for constraint patches
    #includeEtc "caseDicts/setConstraintTypes"

    top
    {
        type            symmetryPlane;
    }
    ground
    {
        type            symmetryPlane;
    }
    inlet
    {
        type            zeroGradient;
    }
    outlet
    {
        type            fixedValue;
	value           uniform 0;
    }
    side
    {
        type            symmetryPlane;
    }
    side2
    {
        type            symmetryPlane;
    }
    airfoil
    {
        type            zeroGradient;
    }
}


